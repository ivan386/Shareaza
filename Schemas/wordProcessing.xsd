			</attribute>

		        <attribute name="formatVersion" type="string"/>
			<attribute name="pages" type="int"/>
		        <attribute name="link" type="uriReference"/>
			<attribute name="comments">
				<simpleType base="string">
					<maxInclusive value="100"/>
				</simpleType>
			</attribute>
		</all>
	</complexType>

</schema>





