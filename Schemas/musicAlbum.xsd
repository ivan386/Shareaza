<?xml version="1.0" encoding="UTF-8"?>

<schema targetNamespace="http://www.shareaza.com/schemas/musicAlbum.xsd" xmlns="http://www.w3.org/2001/XMLSchema">
  
  <element name="musicAlbums">
	<complexType>
		<element name="musicAlbum" type="albumType" minOccurs="1" maxOccurs="unbounded"/>
	</complexType>
  </element>     
  <complexType name="albumType">
	<all>
		<attribute name="title" type="string"/>
		<attribute name="artist" type="string"/>
		<attribute name="year" type="year"/>
		<attribute name="genre">
		<simpleType base="string">
		  <enumeration value="A capella"/>
		  <enumeration value="Acid"/>
		  <enumeration value="Acid Jazz"/>
		  <enumeration value="Acid Punk"/>
		  <enumeration value="Acoustic"/>
