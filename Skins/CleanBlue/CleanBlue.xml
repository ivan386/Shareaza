			<anchor name="Icon" rect="2,2,20,20"/>
		</anchors>
		<caption rect="20,1,-43,18" fontFace="Tahoma" fontSize="10" fontWeight="800" caps="1" colour="404080" inactiveColour="404080"/>
	</windowSkin>
	
	<!-- Popup Windows -->
	<windowSkin>
		<image path="CleanBlue.bmp"/>
		<target window="CDialog"/>
		<target window="CSettingsSheet"/>
		<parts>
			<part name="TopLeft" rect="0,53,20,20"/>
			<part name="Top" rect="20,53,68,20"/>
			<part name="TopRight" rect="113,53,15,20"/>
			<part name="Left" rect="0,17,1,111"/>
			<part name="Right" rect="0,17,1,111"/>
			<part name="Bottom" rect="0,0,128,1"/>
			<part name="CloseHover" rect="117,22,11,11"/>
			<part name="CloseDown" rect="84,22,11,11"/>
		</parts>
		<anchors>
			<anchor name="Icon" rect="2,2,20,20"/>
			<anchor name="System" rect="1,1,15,15"/>
			<anchor name="Close" rect="-14,6,11,11"/>
		</anchors>
		<caption rect="21,4,-43,15" fontFace="Tahoma" fontSize="10" fontWeight="800" caps="1" colour="404080" inactiveColour="C0C0FF"/>
		<region>
			<shape type="rectangle" rect="0,3,-1,-1"/>
			<shape type="rectangle" rect="0,0,20,3" combine="or"/>
		</region>
	</windowSkin>

</windowSkins>

</skin>
